def call_open_interest6a = if strikes < 6 then 0 else if IsNaN(open_interest("." + (if useAutoOptionSymbol then "/E" + AsPrice(exp_week_index) + (if target_dow == 1 then "A" else if target_dow == 2 then "B" else if target_dow == 3 then "C" else if target_dow == 4 then "D" else "W") + (if exp_month == 1 then "F" else if exp_month == 2 then "G" else if exp_month == 3 then "H" else if exp_month == 4 then "J" else if exp_month == 5 then "K" else if exp_month == 6 then "M" else if exp_month == 7 then "N" else if exp_month == 8 then "Q" else if exp_month == 9 then "U" else if exp_month == 10 then "V" else if exp_month == 11 then "X" else "Z") + AsPrice(exp_year_short) else manualOptionSymbol) + "C" + AsPrice(strike_minus6) + ":XCME", period = agg)) then 0 else open_interest("." + (if useAutoOptionSymbol then "/E" + AsPrice(exp_week_index) + (if target_dow == 1 then "A" else if target_dow == 2 then "B" else if target_dow == 3 then "C" else if target_dow == 4 then "D" else "W") + (if exp_month == 1 then "F" else if exp_month == 2 then "G" else if exp_month == 3 then "H" else if exp_month == 4 then "J" else if exp_month == 5 then "K" else if exp_month == 6 then "M" else if exp_month == 7 then "N" else if exp_month == 8 then "Q" else if exp_month == 9 then "U" else if exp_month == 10 then "V" else if exp_month == 11 then "X" else "Z") + AsPrice(exp_year_short) else manualOptionSymbol) + "C" + AsPrice(strike_minus6) + ":XCME", period = agg);
def call_open_interest7a = if strikes < 7 then 0 else if IsNaN(open_interest("." + (if useAutoOptionSymbol then "/E" + AsPrice(exp_week_index) + (if target_dow == 1 then "A" else if target_dow == 2 then "B" else if target_dow == 3 then "C" else if target_dow == 4 then "D" else "W") + (if exp_month == 1 then "F" else if exp_month == 2 then "G" else if exp_month == 3 then "H" else if exp_month == 4 then "J" else if exp_month == 5 then "K" else if exp_month == 6 then "M" else if exp_month == 7 then "N" else if exp_month == 8 then "Q" else if exp_month == 9 then "U" else if exp_month == 10 then "V" else if exp_month == 11 then "X" else "Z") + AsPrice(exp_year_short) else manualOptionSymbol) + "C" + AsPrice(strike_minus7) + ":XCME", period = agg)) then 0 else open_interest("." + (if useAutoOptionSymbol then "/E" + AsPrice(exp_week_index) + (if target_dow == 1 then "A" else if target_dow == 2 then "B" else if target_dow == 3 then "C" else if target_dow == 4 then "D" else "W") + (if exp_month == 1 then "F" else if exp_month == 2 then "G" else if exp_month == 3 then "H" else if exp_month == 4 then "J" else if exp_month == 5 then "K" else if exp_month == 6 then "M" else if exp_month == 7 then "N" else if exp_month == 8 then "Q" else if exp_month == 9 then "U" else if exp_month == 10 then "V" else if exp_month == 11 then "X" else "Z") + AsPrice(exp_year_short) else manualOptionSymbol) + "C" + AsPrice(strike_minus7) + ":XCME", period = agg);
def call_open_interest8a = if strikes < 8 then 0 else if IsNaN(open_interest("." + (if useAutoOptionSymbol then "/E" + AsPrice(exp_week_index) + (if target_dow == 1 then "A" else if target_dow == 2 then "B" else if target_dow == 3 then "C" else if target_dow == 4 then "D" else "W") + (if exp_month == 1 then "F" else if exp_month == 2 then "G" else if exp_month == 3 then "H" else if exp_month == 4 then "J" else if exp_month == 5 then "K" else if exp_month == 6 then "M" else if exp_month == 7 then "N" else if exp_month == 8 then "Q" else if exp_month == 9 then "U" else if exp_month == 10 then "V" else if exp_month == 11 then "X" else "Z") + AsPrice(exp_year_short) else manualOptionSymbol) + "C" + AsPrice(strike_minus8) + ":XCME", period = agg)) then 0 else open_interest("." + (if useAutoOptionSymbol then "/E" + AsPrice(exp_week_index) + (if target_dow == 1 then "A" else if target_dow == 2 then "B" else if target_dow == 3 then "C" else if target_dow == 4 then "D" else "W") + (if exp_month == 1 then "F" else if exp_month == 2 then "G" else if exp_month == 3 then "H" else if exp_month == 4 then "J" else if exp_month == 5 then "K" else if exp_month == 6 then "M" else if exp_month == 7 then "N" else if exp_month == 8 then "Q" else if exp_month == 9 then "U" else if exp_month == 10 then "V" else if exp_month == 11 then "X" else "Z") + AsPrice(exp_year_short) else manualOptionSymbol) + "C" + AsPrice(strike_minus8) + ":XCME", period = agg);
def call_open_interest9a = if strikes < 9 then 0 else if IsNaN(open_interest("." + (if useAutoOptionSymbol then "/E" + AsPrice(exp_week_index) + (if target_dow == 1 then "A" else if target_dow == 2 then "B" else if target_dow == 3 then "C" else if target_dow == 4 then "D" else "W") + (if exp_month == 1 then "F" else if exp_month == 2 then "G" else if exp_month == 3 then "H" else if exp_month == 4 then "J" else if exp_month == 5 then "K" else if exp_month == 6 then "M" else if exp_month == 7 then "N" else if exp_month == 8 then "Q" else if exp_month == 9 then "U" else if exp_month == 10 then "V" else if exp_month == 11 then "X" else "Z") + AsPrice(exp_year_short) else manualOptionSymbol) + "C" + AsPrice(strike_minus9) + ":XCME", period = agg)) then 0 else open_interest("." + (if useAutoOptionSymbol then "/E" + AsPrice(exp_week_index) + (if target_dow == 1 then "A" else if target_dow == 2 then "B" else if target_dow == 3 then "C" else if target_dow == 4 then "D" else "W") + (if exp_month == 1 then "F" else if exp_month == 2 then "G" else if exp_month == 3 then "H" else if exp_month == 4 then "J" else if exp_month == 5 then "K" else if exp_month == 6 then "M" else if exp_month == 7 then "N" else if exp_month == 8 then "Q" else if exp_month == 9 then "U" else if exp_month == 10 then "V" else if exp_month == 11 then "X" else "Z") + AsPrice(exp_year_short) else manualOptionSymbol) + "C" + AsPrice(strike_minus9) + ":XCME", period = agg);
def call_open_interest10a = if strikes < 10 then 0 else if IsNaN(open_interest("." + (if useAutoOptionSymbol then "/E" + AsPrice(exp_week_index) + (if target_dow == 1 then "A" else if target_dow == 2 then "B" else if target_dow == 3 then "C" else if target_dow == 4 then "D" else "W") + (if exp_month == 1 then "F" else if exp_month == 2 then "G" else if exp_month == 3 then "H" else if exp_month == 4 then "J" else if exp_month == 5 then "K" else if exp_month == 6 then "M" else if exp_month == 7 then "N" else if exp_month == 8 then "Q" else if exp_month == 9 then "U" else if exp_month == 10 then "V" else if exp_month == 11 then "X" else "Z") + AsPrice(exp_year_short) else manualOptionSymbol) + "C" + AsPrice(strike_minus10) + ":XCME", period = agg)) then 0 else open_interest("." + (if useAutoOptionSymbol then "/E" + AsPrice(exp_week_index) + (if target_dow == 1 then "A" else if target_dow == 2 then "B" else if target_dow == 3 then "C" else if target_dow == 4 then "D" else "W") + (if exp_month == 1 then "F" else if exp_month == 2 then "G" else if exp_month == 3 then "H" else if exp_month == 4 then "J" else if exp_month == 5 then "K" else if exp_month == 6 then "M" else if exp_month == 7 then "N" else if exp_month == 8 then "Q" else if exp_month == 9 then "U" else if exp_month == 10 then "V" else if exp_month == 11 then "X" else "Z") + AsPrice(exp_year_short) else manualOptionSymbol) + "C" + AsPrice(strike_minus10) + ":XCME", period = agg);

### Gamma
def strike_base_gamma = (Exp(-(Sqr((Log(S / strike_base) + Sqr_IV_2) / Sqrt_t_iv) / 2)) / Sqrt_pi) / Sqrt_t_iv_S;
#AddLabel(yes, base_strike + "_gamma: " + strike_base_gamma);
def strike_plus1_gamma = (Exp(-(Sqr((Log(S / strike_plus1) + Sqr_IV_2) / Sqrt_t_iv) / 2)) / Sqrt_pi) / Sqrt_t_iv_S;
#AddLabel(yes, strike_plus1 + "_gamma: " + strike_plus1_gamma);
def strike_plus2_gamma = (Exp(-(Sqr((Log(S / strike_plus2) + Sqr_IV_2) / Sqrt_t_iv) / 2)) / Sqrt_pi) / Sqrt_t_iv_S;
def strike_plus3_gamma = (Exp(-(Sqr((Log(S / strike_plus3) + Sqr_IV_2) / Sqrt_t_iv) / 2)) / Sqrt_pi) / Sqrt_t_iv_S;
def strike_plus4_gamma = (Exp(-(Sqr((Log(S / strike_plus4) + Sqr_IV_2) / Sqrt_t_iv) / 2)) / Sqrt_pi) / Sqrt_t_iv_S;
def strike_plus5_gamma = (Exp(-(Sqr((Log(S / strike_plus5) + Sqr_IV_2) / Sqrt_t_iv) / 2)) / Sqrt_pi) / Sqrt_t_iv_S;
def strike_plus6_gamma = (Exp(-(Sqr((Log(S / strike_plus6) + Sqr_IV_2) / Sqrt_t_iv) / 2)) / Sqrt_pi) / Sqrt_t_iv_S;
def strike_plus7_gamma = (Exp(-(Sqr((Log(S / strike_plus7) + Sqr_IV_2) / Sqrt_t_iv) / 2)) / Sqrt_pi) / Sqrt_t_iv_S;
def strike_plus8_gamma = (Exp(-(Sqr((Log(S / strike_plus8) + Sqr_IV_2) / Sqrt_t_iv) / 2)) / Sqrt_pi) / Sqrt_t_iv_S;
def strike_plus9_gamma = (Exp(-(Sqr((Log(S / strike_plus9) + Sqr_IV_2) / Sqrt_t_iv) / 2)) / Sqrt_pi) / Sqrt_t_iv_S;
def strike_plus10_gamma = (Exp(-(Sqr((Log(S / strike_plus10) + Sqr_IV_2) / Sqrt_t_iv) / 2)) / Sqrt_pi) / Sqrt_t_iv_S;

###
def strike_minus1_gamma = (Exp(-(Sqr((Log(S / strike_minus1) + Sqr_IV_2) / Sqrt_t_iv) / 2)) / Sqrt_pi) / Sqrt_t_iv_S;
#AddLabel(yes, strike_minus1 + "_gamma: " + strike_minus1_gamma);
def strike_minus2_gamma = (Exp(-(Sqr((Log(S / strike_minus2) + Sqr_IV_2) / Sqrt_t_iv) / 2)) / Sqrt_pi) / Sqrt_t_iv_S;
#AddLabel(yes, strike_minus2 + "_gamma: " + strike_minus2_gamma);
def strike_minus3_gamma = (Exp(-(Sqr((Log(S / strike_minus3) + Sqr_IV_2) / Sqrt_t_iv) / 2)) / Sqrt_pi) / Sqrt_t_iv_S;
def strike_minus4_gamma = (Exp(-(Sqr((Log(S / strike_minus4) + Sqr_IV_2) / Sqrt_t_iv) / 2)) / Sqrt_pi) / Sqrt_t_iv_S;
def strike_minus5_gamma = (Exp(-(Sqr((Log(S / strike_minus5) + Sqr_IV_2) / Sqrt_t_iv) / 2)) / Sqrt_pi) / Sqrt_t_iv_S;
def strike_minus6_gamma = (Exp(-(Sqr((Log(S / strike_minus6) + Sqr_IV_2) / Sqrt_t_iv) / 2)) / Sqrt_pi) / Sqrt_t_iv_S;
def strike_minus7_gamma = (Exp(-(Sqr((Log(S / strike_minus7) + Sqr_IV_2) / Sqrt_t_iv) / 2)) / Sqrt_pi) / Sqrt_t_iv_S;
def strike_minus8_gamma = (Exp(-(Sqr((Log(S / strike_minus8) + Sqr_IV_2) / Sqrt_t_iv) / 2)) / Sqrt_pi) / Sqrt_t_iv_S;
def strike_minus9_gamma = (Exp(-(Sqr((Log(S / strike_minus9) + Sqr_IV_2) / Sqrt_t_iv) / 2)) / Sqrt_pi) / Sqrt_t_iv_S;
def strike_minus10_gamma = (Exp(-(Sqr((Log(S / strike_minus10) + Sqr_IV_2) / Sqrt_t_iv) / 2)) / Sqrt_pi) / Sqrt_t_iv_S;

### GEX
def strike_base_gex = Round((strike_base_gamma * (call_open_interest - putopen_interest) * S) / 10000, 0);
#addlabel(1, "strike_base_gex: " + strike_base_gex, color.green);
def strike_plus1_gex = Round((strike_plus1_gamma * (call_open_interest1 - putopen_interest1a) * S) / 10000, 0);



def strike_plus2_gex =  Round((strike_plus2_gamma * (call_open_interest2 - putopen_interest2a) * S) / 10000, 0);
def strike_plus3_gex =  Round((strike_plus3_gamma * (call_open_interest3 - putopen_interest3a) * S) / 10000, 0);
def strike_plus4_gex =  Round((strike_plus4_gamma * (call_open_interest4 - putopen_interest4a) * S) / 10000, 0);
def strike_plus5_gex =  Round((strike_plus5_gamma * (call_open_interest5 - putopen_interest5a) * S) / 10000, 0);
def strike_plus6_gex =  Round((strike_plus6_gamma * (call_open_interest6 - putopen_interest6a) * S) / 10000, 0);
def strike_plus7_gex =  Round((strike_plus7_gamma * (call_open_interest7 - putopen_interest7a) * S) / 10000, 0);
def strike_plus8_gex =  Round((strike_plus8_gamma * (call_open_interest8 - putopen_interest8a) * S) / 10000, 0);
def strike_plus9_gex =  Round((strike_plus9_gamma * (call_open_interest9 - putopen_interest9a) * S) / 10000, 0);
def strike_plus10_gex =  Round((strike_plus10_gamma * (call_open_interest10 - putopen_interest10a) * S) / 10000, 0);

def strike_minus1_gex =  Round((strike_minus1_gamma * (call_open_interest1a - putopen_interest1) * S) / 10000, 0);
#addlabel(1, " strike_minus1_gex: " +  strike_minus1_gex , color.orange);
def strike_minus2_gex =  Round((strike_minus2_gamma * (call_open_interest2a - putopen_interest2) * S) / 10000, 0);
def strike_minus3_gex =  Round((strike_minus3_gamma * (call_open_interest3a - putopen_interest3) * S) / 10000, 0);
def strike_minus4_gex =  Round((strike_minus4_gamma * (call_open_interest4a - putopen_interest4) * S) / 10000, 0);
def strike_minus5_gex =  Round((strike_minus5_gamma * (call_open_interest5a - putopen_interest5) * S) / 10000, 0);
def strike_minus6_gex =  Round((strike_minus6_gamma * (call_open_interest6a - putopen_interest6) * S) / 10000, 0);
def strike_minus7_gex =  Round((strike_minus7_gamma * (call_open_interest7a - putopen_interest7) * S) / 10000, 0);
def strike_minus8_gex =  Round((strike_minus8_gamma * (call_open_interest8a - putopen_interest8) * S) / 10000, 0);
def strike_minus9_gex =  Round((strike_minus9_gamma * (call_open_interest9a - putopen_interest9) * S) / 10000, 0);
def strike_minus10_gex =  Round((strike_minus10_gamma * (call_open_interest10a - putopen_interest10) * S) / 10000, 0);

#####################################

def shift_line_right = 5;

def lastbar = if IsNaN(close[-1]) and !IsNaN(close)  
              then BarNumber()  
              else lastbar[1]; 

####################

def minGEX0 = Min(
        Min(Min(strike_base_gex, strike_plus1_gex), Min(strike_plus2_gex, strike_plus3_gex)),
        Min(Min(strike_plus4_gex, strike_plus5_gex), Min(strike_plus6_gex, strike_plus7_gex))
);
def minGEX1 = Min(
        Min(Min(strike_minus1_gex, strike_minus2_gex), Min(strike_minus3_gex, strike_minus4_gex)),
        Min(Min(strike_minus5_gex, strike_minus6_gex), Min(strike_minus7_gex, strike_minus8_gex))
);
def minGEX = Min(minGEX0, minGEX1);

AddLabel(yes, "Min GEX: " + minGEX, color.Magenta);


def minGEXStrike = if minGEX == strike_base_gex then strike_base
                   else if minGEX == strike_plus1_gex then strike_plus1
                   else if minGEX == strike_plus2_gex then strike_plus2
                   else if minGEX == strike_plus3_gex then strike_plus3
                   else if minGEX == strike_plus4_gex then strike_plus4
                   else if minGEX == strike_plus5_gex then strike_plus5
                   else if minGEX == strike_plus6_gex then strike_plus6
                   else if minGEX == strike_plus7_gex then strike_plus7
                   else if minGEX == strike_minus1_gex then strike_minus1
                   else if minGEX == strike_minus2_gex then strike_minus2
                   else if minGEX == strike_minus3_gex then strike_minus3
                   else if minGEX == strike_minus4_gex then strike_minus4
                   else if minGEX == strike_minus5_gex then strike_minus5
                   else if minGEX == strike_minus6_gex then strike_minus6
                   else if minGEX == strike_minus7_gex then strike_minus7
                   else strike_minus8;

def maxGEX0 = Max(
        Max(Max(strike_base_gex, strike_plus1_gex), Max(strike_plus2_gex, strike_plus3_gex)),
        Max(Max(strike_plus4_gex, strike_plus5_gex), Max(strike_plus6_gex, strike_plus7_gex))
);
def maxGEX1 = Max(
        Max(Max(strike_minus1_gex, strike_minus2_gex), Max(strike_minus3_gex, strike_minus4_gex)),
        Max(Max(strike_minus5_gex, strike_minus6_gex), Max(strike_minus7_gex, strike_minus8_gex))
);
def maxGEX = Max(maxGEX0, maxGEX1);

AddLabel(yes, "Max GEX: " + maxGEX, color. CYAN);

# Identify the associated strike
def maxGEXStrike = if maxGEX == strike_base_gex then strike_base
                   else if maxGEX == strike_plus1_gex then strike_plus1
                   else if maxGEX == strike_plus2_gex then strike_plus2
                   else if maxGEX == strike_plus3_gex then strike_plus3
                   else if maxGEX == strike_plus4_gex then strike_plus4
                   else if maxGEX == strike_plus5_gex then strike_plus5
                   else if maxGEX == strike_plus6_gex then strike_plus6
                   else if maxGEX == strike_plus7_gex then strike_plus7
                   else if maxGEX == strike_minus1_gex then strike_minus1
                   else if maxGEX == strike_minus2_gex then strike_minus2
                   else if maxGEX == strike_minus3_gex then strike_minus3
                   else if maxGEX == strike_minus4_gex then strike_minus4
                   else if maxGEX == strike_minus5_gex then strike_minus5
                   else if maxGEX == strike_minus6_gex then strike_minus6
                   else if maxGEX == strike_minus7_gex then strike_minus7
                   else strike_minus8;


######### 
AddChartBubble(BarNumber() == HighestAll(lastbar + shift_line_right -1) and AbsValue(strike_base_gex[shift_line_right-1]) > 0 and strikes > 0, strike_base, strike_base_gex[shift_line_right-1], if strike_base_gex[shift_line_right-1] == Maxgex[shift_line_right-1] then color.Cyan else if strike_base_gex[shift_line_right-1] == Mingex[shift_line_right-1]  then color.magenta else  if strike_base_gex[shift_line_right-1] > 0 then Color.GREEN else Color.RED, if strike_base_gex[shift_line_right-1] > 0 then yes else no);
AddChartBubble(BarNumber() == HighestAll(lastbar + shift_line_right -1) and AbsValue(strike_plus1_gex[shift_line_right-1]) > 0 and strikes > 0, strike_plus1, strike_plus1_gex[shift_line_right-1], if strike_plus1_gex[shift_line_right-1] == Maxgex[shift_line_right-1] then color.Cyan else if strike_plus1_gex[shift_line_right-1] == Mingex[shift_line_right-1]  then color.magenta else  if strike_plus1_gex[shift_line_right-1] > 0 then Color.GREEN else Color.RED, if strike_plus1_gex[shift_line_right-1] > 0 then yes else no);
AddChartBubble(BarNumber() == HighestAll(lastbar + shift_line_right -1) and AbsValue(strike_plus2_gex[shift_line_right-1]) > 0 and strikes > 0, strike_plus2, strike_plus2_gex[shift_line_right-1], if strike_plus2_gex[shift_line_right-1] == Maxgex[shift_line_right-1] then color.Cyan else if strike_plus2_gex[shift_line_right-1] == Mingex[shift_line_right-1]  then color.magenta else  if strike_plus2_gex[shift_line_right-1] > 0 then Color.GREEN else Color.RED, if strike_plus2_gex[shift_line_right-1] > 0 then yes else no);
