# Gex Chart Upper /ES Jan 13, 2026
# Twitter @2187Nick
# Work in progress
# Need to address how its capturing the center strike
# and how to calculate hours left in day.

declare upper;
declare once_per_bar;
input useAutoOptionSymbol = yes;
input manualOptionSymbol = "/EW3F26";
input strikeSpacing = 5.0;
input strikes = 10;

input manualcenterstrike = {default "false", "true"};
input centerstrike = 0;

input ManuallySetExpiration = {default "false", "true"};
input Expiration_YYMMDD = 2606030;
def agg = AggregationPeriod.DAY;

#def seconds_left = SecondsTillTime(1615);
#def hours_left = seconds_left / 3600;
#def days_left = if hours_left <= 0 then 0 else hours_left / 24;
# setting this to 1 for now. Need to think through the logic to get it to auto calculate
# hours lef
def days_left = 1;

#def IV = SeriesVolatility(series = 1);
def IV = imp_volatility("/ES");
#AddLabel(yes, "IV = " + IV);

def CurrentYear = GetYear() - 2000;
def CurrentMonth = GetMonth();
def CurrentDOM = GetDayOfMonth(GetYYYYMMDD());
def DateString_auto = GetYYYYMMDD() - 20000000;
def DateString = if ManuallySetExpiration then Expiration_YYMMDD else  DateString_auto;

# --- Auto option root calculation for /ES options ---
# We derive the root from the *current chart day*.
# ES options now have expirations every weekday, so we build the contract for "today".
def yyyymmdd_today = GetYYYYMMDD();
def today_year = GetYear();
def today_month = GetMonth();
def today_dom = GetDayOfMonth(yyyymmdd_today);
def today_dow_raw = GetDayOfWeek(GetYYYYMMDD());

# Use today's DOW as the contract day. (Mon=1..Fri=5). If weekend, fall back to Friday.
def contract_dow = if today_dow_raw <= 5 then today_dow_raw else 5;

# Backward-compatible alias used throughout the OI symbol builders
def target_dow = contract_dow;

# Expiration date = today (chart day)
def exp_dom = today_dom;
def exp_month = today_month;
def exp_year = today_year;
def exp_yyyymmdd = yyyymmdd_today;
def exp_year_short = exp_year % 100;

# Week index is based on the contract weekday within the current month.
def exp_year_month = exp_year * 100 + exp_month;
def exp_first_dow_raw = GetDayOfWeek(exp_year_month * 100 + 1);
def exp_first_dow = if 1 then exp_first_dow_raw else (if exp_first_dow_raw == 1 then 7 else exp_first_dow_raw - 1);
def daysFromMonthStartToTarget = if exp_first_dow <= contract_dow then contract_dow - exp_first_dow else 7 - (exp_first_dow - contract_dow);
def first_target_dom = 1 + daysFromMonthStartToTarget;

# Integer division via Ceil: floor(n/7) == Ceil((n+1)/7)-1  => weekIndex == Ceil((n+1)/7)
def exp_week_index = Ceil((exp_dom - first_target_dom + 1) / 7);

def S = close(period = agg);
#addlabel(1, "close: " + S, color.white);
#addlabel(1, "open: " +  open(period = agg), color.cyan);
def r = 0;
def t = ((DateString - DateString_auto) + days_left) / 365;
#def t = .005;
#addlabel(1, "time left: " + t, color.green);
def Sqr_IV_2 = Sqr(IV) / 2 * t;
def Sqrt_t_iv = IV * Sqrt(t);
def Sqrt_pi = Sqrt(2 * Double.Pi);
def Sqrt_t_iv_S = S * IV * Sqrt(t);

DefineGlobalColor("CallColor", Color.GREEN);
DefineGlobalColor("PutColor", Color.RED);
AddLabel(yes,
        if useAutoOptionSymbol then
                "/E" +
                AsPrice(exp_week_index) +
                (if contract_dow == 1 then "A" else if contract_dow == 2 then "B" else if contract_dow == 3 then "C" else if contract_dow == 4 then "D" else "W") +
                (if exp_month == 1 then "F" else if exp_month == 2 then "G" else if exp_month == 3 then "H" else if exp_month == 4 then "J" else if exp_month == 5 then "K" else if exp_month == 6 then "M" else if exp_month == 7 then "N" else if exp_month == 8 then "Q" else if exp_month == 9 then "U" else if exp_month == 10 then "V" else if exp_month == 11 then "X" else "Z") +
                AsPrice(exp_year_short)
        else manualOptionSymbol,
        GlobalColor("CallColor"));
AddLabel(yes, AsPrice(DateString) + "C", GlobalColor("CallColor"));
AddLabel(yes,
        if useAutoOptionSymbol then
                "/E" +
                AsPrice(exp_week_index) +
                (if contract_dow == 1 then "A" else if contract_dow == 2 then "B" else if contract_dow == 3 then "C" else if contract_dow == 4 then "D" else "W") +
                (if exp_month == 1 then "F" else if exp_month == 2 then "G" else if exp_month == 3 then "H" else if exp_month == 4 then "J" else if exp_month == 5 then "K" else if exp_month == 6 then "M" else if exp_month == 7 then "N" else if exp_month == 8 then "Q" else if exp_month == 9 then "U" else if exp_month == 10 then "V" else if exp_month == 11 then "X" else "Z") +
                AsPrice(exp_year_short)
        else manualOptionSymbol,
        GlobalColor("PutColor"));
AddLabel(yes, AsPrice(DateString) + "P", GlobalColor("PutColor"));
#AddLabel(useAutoOptionSymbol, "Exp: " + exp_yyyymmdd, Color.GRAY);

def openlevel = if manualcenterstrike then centerstrike else open(period = agg);
def rounding_factor = if strikeSpacing > 1 and strikeSpacing < 25 then -1 else if strikeSpacing > 24 then -2 else 0;
def floor_or_ceiling =  Round(openlevel, rounding_factor);
def base_strike = floor_or_ceiling;
#addlabel(yes, "base_strike: " + base_strike);

def strike_base = base_strike;
def strike_plus1 = base_strike + strikeSpacing;
def strike_plus2 = base_strike + strikeSpacing * 2;
def strike_plus3 = base_strike + strikeSpacing * 3;
def strike_plus4 = base_strike + strikeSpacing * 4;
def strike_plus5 = base_strike + strikeSpacing * 5;
def strike_plus6 = base_strike + strikeSpacing * 6;
def strike_plus7 = base_strike + strikeSpacing * 7;
def strike_plus8 = base_strike + strikeSpacing * 8;
def strike_plus9 = base_strike + strikeSpacing * 9;
def strike_plus10 = base_strike + strikeSpacing * 10;

def strike_minus1 = base_strike - strikeSpacing;
def strike_minus2 = base_strike - strikeSpacing * 2;
def strike_minus3 = base_strike - strikeSpacing * 3;
def strike_minus4 = base_strike - strikeSpacing * 4;
def strike_minus5 = base_strike - strikeSpacing * 5;
def strike_minus6 = base_strike - strikeSpacing * 6;
def strike_minus7 = base_strike - strikeSpacing * 7;
def strike_minus8 = base_strike - strikeSpacing * 8;
def strike_minus9 = base_strike - strikeSpacing * 9;
def strike_minus10 = base_strike - strikeSpacing * 10;

### Put Option open_interest
def putopen_interest = if IsNaN(open_interest("." + (if useAutoOptionSymbol then "/E" + AsPrice(exp_week_index) + (if target_dow == 1 then "A" else if target_dow == 2 then "B" else if target_dow == 3 then "C" else if target_dow == 4 then "D" else "W") + (if exp_month == 1 then "F" else if exp_month == 2 then "G" else if exp_month == 3 then "H" else if exp_month == 4 then "J" else if exp_month == 5 then "K" else if exp_month == 6 then "M" else if exp_month == 7 then "N" else if exp_month == 8 then "Q" else if exp_month == 9 then "U" else if exp_month == 10 then "V" else if exp_month == 11 then "X" else "Z") + AsPrice(exp_year_short) else manualOptionSymbol) + "P" + AsPrice(strike_base) + ":XCME", period = agg)) then 0 else open_interest("." + (if useAutoOptionSymbol then "/E" + AsPrice(exp_week_index) + (if target_dow == 1 then "A" else if target_dow == 2 then "B" else if target_dow == 3 then "C" else if target_dow == 4 then "D" else "W") + (if exp_month == 1 then "F" else if exp_month == 2 then "G" else if exp_month == 3 then "H" else if exp_month == 4 then "J" else if exp_month == 5 then "K" else if exp_month == 6 then "M" else if exp_month == 7 then "N" else if exp_month == 8 then "Q" else if exp_month == 9 then "U" else if exp_month == 10 then "V" else if exp_month == 11 then "X" else "Z") + AsPrice(exp_year_short) else manualOptionSymbol) + "P" + AsPrice(strike_base) + ":XCME", period = agg);
#addlabel(1, "putopen_interest: " + putopen_interest, color.yellow);
def putopen_interest1 =  if strikes < 1 then 0 else if IsNaN(open_interest("." + (if useAutoOptionSymbol then "/E" + AsPrice(exp_week_index) + (if target_dow == 1 then "A" else if target_dow == 2 then "B" else if target_dow == 3 then "C" else if target_dow == 4 then "D" else "W") + (if exp_month == 1 then "F" else if exp_month == 2 then "G" else if exp_month == 3 then "H" else if exp_month == 4 then "J" else if exp_month == 5 then "K" else if exp_month == 6 then "M" else if exp_month == 7 then "N" else if exp_month == 8 then "Q" else if exp_month == 9 then "U" else if exp_month == 10 then "V" else if exp_month == 11 then "X" else "Z") + AsPrice(exp_year_short) else manualOptionSymbol) + "P" + AsPrice(strike_minus1) + ":XCME", period = agg)) then 0 else open_interest("." + (if useAutoOptionSymbol then "/E" + AsPrice(exp_week_index) + (if target_dow == 1 then "A" else if target_dow == 2 then "B" else if target_dow == 3 then "C" else if target_dow == 4 then "D" else "W") + (if exp_month == 1 then "F" else if exp_month == 2 then "G" else if exp_month == 3 then "H" else if exp_month == 4 then "J" else if exp_month == 5 then "K" else if exp_month == 6 then "M" else if exp_month == 7 then "N" else if exp_month == 8 then "Q" else if exp_month == 9 then "U" else if exp_month == 10 then "V" else if exp_month == 11 then "X" else "Z") + AsPrice(exp_year_short) else manualOptionSymbol) + "P" + AsPrice(strike_minus1) + ":XCME", period = agg);
